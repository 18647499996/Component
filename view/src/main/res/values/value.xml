    <declare-styleable name="ADButton">
        <attr name="liu_is_circle" />
        <attr name="liu_round_corner" />
        <attr name="liu_round_corner_top_left" />
        <attr name="liu_round_corner_top_right" />
        <attr name="liu_round_corner_bottom_left" />
        <attr name="liu_round_corner_bottom_right" />
        <attr name="liu_stroke_color" />
        <attr name="liu_stroke_width" />
        <attr name="liu_clip_background" />
    </declare-styleable>

    <declare-styleable name="ADCountDownButton">
        <attr name="liu_millis_as_future" format="integer" />
        <attr name="liu_count_down_interval" format="integer" />
    </declare-styleable>

    <declare-styleable name="ADRecyclerView">
        <attr name="liu_is_circle" />
        <attr name="liu_round_corner" />
        <attr name="liu_round_corner_top_left" />
        <attr name="liu_round_corner_top_right" />
        <attr name="liu_round_corner_bottom_left" />
        <attr name="liu_round_corner_bottom_right" />
        <attr name="liu_stroke_color" />
        <attr name="liu_stroke_width" />
        <attr name="liu_clip_background" />
        <attr name="liu_layout_manager" format="enum">
            <enum name="linear" value="1" />
            <enum name="grid" value="2" />
            <enum name="storage" value="3" />
            <enum name="center" value="4" />
            <enum name="flow" value="5" />
        </attr>
        <attr name="liu_orientation" format="enum">
            <enum name="Vertical" value="1" />
            <enum name="Horizontal" value="0" />
        </attr>
        <attr name="liu_span_count" format="integer" />
    </declare-styleable>

    <declare-styleable name="ADCellTextLayout">
        <attr name="liu_left_text" format="string" />
        <attr name="liu_left_text_color" format="color" />
        <attr name="liu_left_text_size" format="dimension" />
        <attr name="liu_left_text_bold" format="boolean" />
        <attr name="liu_left_drawable" format="reference" />
        <attr name="liu_left_drawable_width" format="dimension" />
        <attr name="liu_left_drawable_height" format="dimension" />
        <attr name="liu_left_drawable_padding" format="dimension" />
        <attr name="liu_left_drawable_direction" format="enum">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
        <attr name="liu_sub_text" format="string" />
        <attr name="liu_sub_text_color" format="color" />
        <attr name="liu_sub_text_size" format="dimension" />
        <attr name="liu_sub_text_bold" format="boolean" />
        <attr name="liu_sub_drawable" format="reference" />
        <attr name="liu_sub_drawable_width" format="dimension" />
        <attr name="liu_sub_drawable_height" format="dimension" />
        <attr name="liu_sub_drawable_padding" format="dimension" />
        <attr name="liu_sub_left_margin" format="dimension" />
        <attr name="liu_sub_right_margin" format="dimension" />
        <attr name="liu_sub_drawable_direction" format="enum">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
        <attr name="liu_right_text" format="string" />
        <attr name="liu_right_text_color" format="color" />
        <attr name="liu_right_text_size" format="dimension" />
        <attr name="liu_right_text_bold" format="boolean" />
        <attr name="liu_right_drawable" format="reference" />
        <attr name="liu_right_drawable_width" format="dimension" />
        <attr name="liu_right_drawable_height" format="dimension" />
        <attr name="liu_right_drawable_padding" format="dimension" />
        <attr name="liu_right_drawable_direction" format="enum">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>

        <attr name="liu_line_left_margin" format="dimension" />
        <attr name="liu_line_right_margin" format="dimension" />
        <attr name="liu_line_height" format="dimension" />
        <attr name="liu_line_background_color" format="color" />
        <attr name="liu_line_visibility" format="enum">
            <enum name="visibility" value="1" />
            <enum name="gone" value="2" />
        </attr>

    </declare-styleable>

    <declare-styleable name="ADCircleProgress">
        <attr name="liu_finished_color" format="color" />
        <attr name="liu_unfinished_color" format="color" />
        <attr name="liu_finished_stroke_width" format="dimension" />
        <attr name="liu_unfinished_stroke_width" format="dimension" />
        <attr name="liu_text" format="string" />
        <attr name="liu_text_size" format="dimension" />
        <attr name="liu_text_color" format="color" />
        <attr name="liu_clock_wise" format="boolean" />
        <attr name="liu_progress" format="float" />
        <attr name="liu_max" format="integer" />
        <attr name="liu_starting_angle" format="integer" />
        <attr name="liu_is_auto_loading_animation" format="boolean" />
    </declare-styleable>


    <declare-styleable name="ADIndicatorTab">
        <attr name="liu_indicator_text_size" format="dimension" />
        <attr name="liu_indicator_text_height" format="dimension" />
        <attr name="liu_indicator_empty_color" format="color" />
        <attr name="liu_indicator_selected_color" format="color" />
