        <attr name="liu_divider_height" format="dimension" />
        <attr name="liu_insert_radius" format="dimension" />
        <attr name="liu_insert_hint" format="string" />
        <attr name="liu_insert_hint_color" format="color" />
        <attr name="liu_insert_background_color" format="color"/>

    </declare-styleable>

    <bool name="sb__is_swipeable">false</bool>

    <!--dialog弹窗-->
    <style name="ADDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <!-- <item name="android:windowBackground">@drawable/u39_normal</item> -->
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- <item name="android:background">#00000000</item> -->
        <!-- 设置自定义布局的背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 设置window背景透明，也就是去边框 -->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
    </style>

</resources>