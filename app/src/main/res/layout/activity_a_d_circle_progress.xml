<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context=".circleprogress.ADCircleProgressActivity">

    <include layout="@layout/ad_title_builder" />

    <com.liudonghan.view.progress.ADCircleProgress
        android:id="@+id/circle"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:layout_marginTop="15dp"
        app:liu_finished_color="@color/color_ff7900"
        app:liu_finished_stroke_width="3dp"
        app:liu_is_auto_loading_animation="true"
        app:liu_text_color="@color/color_342e2e"
        app:liu_text_size="12sp"
        app:liu_unfinished_color="@color/color_dddddd"
        app:liu_unfinished_stroke_width="3dp" />

</LinearLayout>